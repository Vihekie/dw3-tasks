package POO;

import java.util.Scanner;

public class EX1 {
		public static void main(String[] args){
			int number1 = 0;
			int number2 = 1;
			int soma = 0;
			Scanner inp = new Scanner(System.in);
			int n = inp.nextInt();
			System.out.println(number1);
			System.out.println(number2);
			for(int i = 0; i < n-2; i++ ){
				soma = number1+ number2;
				number1= number2;
				number2 = soma;
				System.out.println(soma);
			}
			System.out.println(soma);
		}
}
